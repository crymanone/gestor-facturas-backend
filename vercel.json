{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/app.py"
        }
    ]
}