{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "crons": [
        {
            "path": "/api/process_queue",
            "schedule": "* * * * *"
        }
    ]
}